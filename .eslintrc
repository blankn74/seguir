{
  "extends": ["airbnb-base", "plugin:import/errors"],
  "rules": {
    "id-length": 0,
    "max-len": 0,
    "consistent-return": 0,
    "arrow-parens": 0,
    "no-underscore-dangle": 0,
    "no-use-before-define": 0,
    "no-param-reassign": 0,
    "no-shadow": 0,
    "no-confusing-arrow": 0,
    "global-require": 0,
    "camelcase": 0,
    "dot-notation": [2, {"allowKeywords": true, "allowPattern": "^[a-z]+$"}]
  }
}